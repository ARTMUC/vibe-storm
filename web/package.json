{
  "name": "vibe-storm-frontend",
  "version": "1.0.0",
  "description": "Frontend assets for VibeStorm application",
  "main": "static/js/main.js",
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack --mode=development --watch",
    "build:css": "postcss static/css/main.css -o static/css/main.min.css",
    "build:react": "esbuild react/index.tsx --bundle --outfile=static/react-bundle.js --minify --global-name=ReactApp",
    "type-check": "tsc --noEmit",
    "lint": "eslint static/js/main.ts react/**/*.tsx",
    "format": "prettier --write static/js/main.ts react/**/*.tsx"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "css-loader": "^6.0.0",
    "eslint": "^8.0.0",
    "esbuild": "^0.19.0",
    "postcss": "^8.0.0",
    "postcss-cli": "^10.0.0",
    "prettier": "^3.0.0",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.0.0",
    "typescript": "^5.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not op_mini all"
  ]
}
